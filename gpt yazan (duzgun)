abc = int(input())
def_ = int(input())

a = abc // 100
b = abc // 10 % 10
c = abc % 10

d = def_ // 100
e = def_ // 10 % 10
f = def_ % 10

print()

if (99 < abc < 1000 and 99 < def_ < 1000):
    # Rəqəmlərin eyniliyini yoxlayır
    unique_abc = {a, b, c}
    unique_def = {d, e, f}
    
    if unique_abc == unique_def:
        print("eyni terkibli reqemler")
    else:
        print("none 3")
else:
    print("3 reqemli eded daxil edin")
